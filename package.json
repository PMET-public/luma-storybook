{
  "name": "luma-storybook",
  "version": "1.0.0",
  "description": "Front-end Components Storybook",
  "repository": "git@github.com:PMET-public/luma-storybook.git",
  "author": "Carlos A. Cabrera <cacabrer@adobe.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.2",
    "yarn": ">=1.15.2"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "predev": "rimraf dist",
    "dev": "concurrently -n rollup,storybook -c cyan,magenta 'yarn rollup:watch' 'yarn storybook:watch'",
    "rollup:watch": "rollup -c -w",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production rollup -c",
    "generate": "./bin/generate.js",
    "storybook:watch": "start-storybook -p 5001 --quiet --ci -s ./public",
    "storybook:build": "build-storybook -s ./public",
    "storybook:deploy": "storybook-to-ghpages",
    "tsc": "tsc"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-jsx": "^3.2.1"
  },
