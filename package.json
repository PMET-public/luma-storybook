{
  "name": "luma-storybook",
  "version": "1.0.0",
  "description": "Front-end Components Storybook",
  "repository": "git@github.com:PMET-public/luma-storybook.git",
  "author": "Carlos A. Cabrera <cacabrer@adobe.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.2",
    "yarn": ">=1.15.2"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "predev": "rimraf dist",
    "dev": "concurrently -n webpack,storybook -c cyan,magenta 'yarn webpack-watch' 'yarn storybook-watch'",
    "webpack-watch": "webpack --watch --preserveWatchOutput --mode=production",
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "prepare": "yarn build",
    "generate": "./bin/generate.js",
    "storybook-watch": "start-storybook -p 5001 --quiet -s ./src/styles/variables,./public",
    "build-storybook": "build-storybook -s ./src/styles/variables,./public",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.0.10",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-backgrounds": "^5.0.10",
    "@storybook/addon-centered": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-viewport": "^5.0.10",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.10",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/common-tags": "^1.8.0",
    "@types/less": "^3.0.0",
    "@types/node": "^11.13.8",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-centered": "^3.4.0",
    "@types/storybook__addon-knobs": "^4.0.5",
    "@types/storybook__react": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "common-tags": "^1.8.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-github-btn": "^1.0.5",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.16.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {}
}
